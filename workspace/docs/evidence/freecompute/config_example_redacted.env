# FreeComputeCloud â€” Example Configuration (REDACTED)
# Copy to your .env or export in your shell. Never commit real keys.

# Master switch (0=off, 1=on)
ENABLE_FREECOMPUTE_CLOUD=0

# Local vLLM
ENABLE_LOCAL_VLLM=0
OPENCLAW_VLLM_BASE_URL=http://127.0.0.1:18888/v1
OPENCLAW_VLLM_API_KEY=[REDACTED]

# Gemini
OPENCLAW_GEMINI_API_KEY=[REDACTED]

# Groq
OPENCLAW_GROQ_API_KEY=[REDACTED]

# Qwen / Alibaba Model Studio
OPENCLAW_QWEN_API_KEY=[REDACTED]
OPENCLAW_QWEN_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1

# OpenRouter
OPENCLAW_OPENROUTER_API_KEY=[REDACTED]

# Provider filtering
FREECOMPUTE_PROVIDER_ALLOWLIST=
FREECOMPUTE_PROVIDER_DENYLIST=

# Global caps
FREECOMPUTE_MAX_DAILY_REQUESTS=200
FREECOMPUTE_MAX_RPM=30
FREECOMPUTE_MAX_DAILY_TOKENS=2000000

# Circuit breaker
FREECOMPUTE_CB_FAILURES=3
FREECOMPUTE_CB_OPEN_SECONDS=120
FREECOMPUTE_CB_TIMEOUT_MS=30000

# Ledger
FREECOMPUTE_LEDGER_PATH=.tmp/quota-ledger
FREECOMPUTE_LEDGER_RESET_HOUR=0
